partial modifier_keys
xkb_symbols "swap" {
  replace key <AC10> { [ colon, semicolon ] };
  replace key <CAPS> { [ Control_L ] };
  replace key <MENU> { [ Super_R ] };

  key <LWIN> { [ Control_L ] };
  modifier_map Control { <LWIN> };

  key <RWIN> { [ Control_R ] };
  modifier_map Control { <RWIN> };


  key <LCTL> { [ Super_L, Super_L ] };
  key <RCTL> { [ Super_R, Super_R ] };
  modifier_map Mod3 { Super_L };
  replace key <LCTL> { [ Super_R, Super_R, ] };
};
